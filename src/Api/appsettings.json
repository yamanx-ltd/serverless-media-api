{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AWS": {
    "Profile": "serverless"
  },
  "ApiKeyValidationSettings": {
    "IsEnabled": false,
    "ApiKey": "1234567",
    "WhiteList": [
      "/swagger"
    ],
    "HeaderName": "x-api-key"
  },
  "UploadSettings": {
    "ExpireTime": 5,
    "BaseFolder": "orginal",
    "BucketName": "cdn.xxxxx.com",
    "DefaultTags": {
      "tag1": "value1",
      "tag2": "value2"
    },
    "AllowedContentTypes": {
      "image/jpeg": "jpg",
      "image/png": "png",
      "image/gif": "gif"
    }
  },
  "EventBusSettings": {
    "IsEnabled": true,
    "TopicArn": "XX"
  },
  "AllowedHosts": "*"
}
